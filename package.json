{
    "name": "paperbits-demo",
    "version": "0.1.526",
    "description": "An example of using Paperbits to build web apps with rich content editing capability.",
    "author": "Paperbits",
    "license": "MIT",
    "keywords": [
        "paperbits",
        "demo"
    ],
    "scripts": {
        "start": "webpack serve --open --config webpack.develop.js",
        "build": "webpack --config webpack.build.js",
        "build-designer": "webpack --config webpack.designer.js",
        "build-publisher": "webpack --config webpack.publisher.js",
        "build-runtime": "webpack --config webpack.runtime.js",
        "publish": "webpack --config webpack.publisher.js && node dist/publisher/index.js && npm run serve-website",
        "serve-website": "webpack serve --open --static ./dist/website --no-stats",
        "lint": "eslint src/**/*.ts"
    },
    "devDependencies": {
        "@types/google.maps": "^3.52.4",
        "@types/knockout.mapping": "^2.0.37",
        "@types/knockout.validation": "0.0.38",
        "@types/mime": "^3.0.1",
        "@typescript-eslint/eslint-plugin": "^5.57.0",
        "@typescript-eslint/parser": "^5.57.0",
        "autoprefixer": "10.4.14",
        "buffer": "^6.0.3",
        "bufferutil": "^4.0.7",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "6.7.3",
        "eslint": "^8.37.0",
        "file-loader": "^6.2.0",
        "html-loader": "^4.2.0",
        "mini-css-extract-plugin": "^2.7.5",
        "postcss-loader": "^7.1.0",
        "raw-loader": "^4.0.2",
        "sass": "^1.60.0",
        "sass-loader": "13.2.2",
        "stream-browserify": "^3.0.0",
        "style-loader": "3.3.2",
        "terser": "5.16.8",
        "terser-webpack-plugin": "5.3.7",
        "ts-loader": "^9.4.2",
        "typescript": "^5.0.3",
        "url-loader": "4.1.1",
        "utf-8-validate": "^6.0.3",
        "webpack": "5.77.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "4.13.2",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "@paperbits/common": "0.1.578",
        "@paperbits/core": "0.1.578",
        "@paperbits/emails": "0.1.578",
        "@paperbits/forms": "0.1.578",
        "@paperbits/gtm": "0.1.578",
        "@paperbits/intercom": "0.1.578",
        "@paperbits/prosemirror": "0.1.578",
        "@paperbits/styles": "0.1.578",
        "@webcomponents/custom-elements": "^1.6.0",
        "core-js": "^3.29.1",
        "knockout": "^3.5.1"
    }
}
